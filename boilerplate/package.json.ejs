{
  "version": "0.1.1", 
  "scripts": {
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force", 
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android", 
    "newclean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i", 
    "start": "node node_modules/react-native/local-cli/cli.js start", 
    "test": "yarn check:locales && yarn check:storybook && yarn flow && yarn jest", 
    "flow": "flow", 
    "flow:watch": "flow-watch", 
    "jest": "jest", 
    "jest:watch": "jest --watch", 
    "xcode": "open ios/<%= props.name %>.xcodeproj", 
    "signing": "cd ios && bundle exec fastlane signing", 
    "prettier": "prettier --write \"src/**/*.js *.js\"", 
    "prettier-storybook": "prettier --write \"storybook/**/*.js\"", 
    "storybook": "storybook start -p 7007", 
    "check:storybook": "scripts/storybook.py check", 
    "check:locales": "scripts/update-i18n-locales.py check", 
    "release": "scripts/internal-release.py", 
    "release:ios": "scripts/internal-release.py ios", 
    "release:android": "scripts/internal-release.py android", 
    "update:storybook": "scripts/storybook.py update", 
    "update:locales": "scripts/update-i18n-locales.py update", 
    "update:bundle-id": "react-native-ci-tools bundle -ia", 
    "update:version": "scripts/set-version.py", 
    "inject-devtools": "remotedev --hostname=localhost --port=8000 --injectserver=reactnative", 
    "disabled-postinstall": "yarn inject-devtools && bundle install && flow-typed install", 
    "precommit": "lint-staged", 
    "prepush": "yarn test"
  }, 
  "lint-staged": {
    "*.js": [
      "prettier --write", 
      "git add"
    ]
  }, 
  "dependencies": {
    "date-fns": "^1.28.5", 
    "invariant": "^2.2.2", 
    "lodash": "^4.17.2", 
    "nodemon": "^1.11.0", 
    "prop-types": "^15.5.10", 
    "querystringify": "^1.0.0", 
    "ramda": "^0.25.0", 
    "react-native-i18n": "^2.0.2", 
    "react-navigation": "^1.0.0", 
    "react-navigation-redux-helpers": "^1.0.1", 
    "react-redux": "^5.0.5", 
    "react": "16.2.0", 
    "redux-persist": "^5.4.0", 
    "redux-saga": "^0.16.0", 
    "redux": "^3.7.1", 
    "remote-redux-devtools": "^0.5.12", 
    "reselect": "^3.0.1"
  }, 
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.2.16", 
    "@storybook/addons": "^3.2.16", 
    "@storybook/channels": "^3.2.16", 
    "@storybook/react-native": "^3.2.16", 
    "babel-core": "^6.0.0", 
    "babel-jest": "^22.4.0", 
    "babel-plugin-root-import": "^5.1.0", 
    "babel-preset-es2015": "^6.18.0", 
    "babel-preset-react-native": "^4.0.0", 
    "enzyme": "^3.2.0", 
    "fetch-mock": "^6.0.0", 
    "flow-bin": "^0.63.0", 
    "flow-typed": "^2.2.3", 
    "flow-watch": "^1.1.1", 
    "husky": "^0.14.3", 
    "jest": "^22.4.0", 
    "jest-static-stubs": "^0.0.1", 
    "lint-staged": "^7.0.0", 
    "node-fetch-polyfill": "^2.0.6", 
    "prettier": "^1.5.2", 
    "react-dom": "^16.0.0", 
    "react-test-renderer": "^16.1.0", 
    "redux-saga-tester": "^1.0.298", 
    "remotedev-server": "^0.2.4", 
    "timekeeper": "^2.0.0"
  }, 
  "jest": {
    "preset": "react-native", 
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ], 
    "moduleNameMapper": {
      ".+\\.(png)$": "jest-static-stubs/png"
    }, 
    "testPathIgnorePatterns": [
      "e2e", 
      "node_modules"
    ], 
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  }, 
  "config": {}
}